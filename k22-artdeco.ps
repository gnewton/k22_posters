/k22{
    gsave
    .3 .3 0  setrgbcolor
    1 setlinecap
    1 setlinejoin
    3 setmiterlimit
    %% I forget where I found this. Unable to properly cite. Not my work/copyright:-(
    %% Start external work
    /inch { 72 mul } def        %  Three Barleycorns

/Functiondict 8 dict def        %  dictionary for shading function
/Shadingdict  8 dict def        %  dictionary for shading definition
/Patterndict  8 dict def        %  dictionary for pattern definition

Functiondict begin              %  function dictionary to dictionary
stack
    /FunctionType 2 def         %  exponential interpolation
    /Domain [ 0.0 1.0 ] def     %  standard 0 -- domain
    /C0 [0 0 0] def       %  magenta at 0
    /C1 [0 .7 0] def       %  cyan at 1
    /N .9 def                  %  exponentiate to power 1.5
end                             %  function dictionary off of dictionary
stack

Shadingdict begin               %  shading dictionary to dictionary
stack
    /ShadingType 2 def          %  shading type 2 is axial shading
    /Coords [ 4.25 inch 0 4.25 inch 11 inch ] def  %  letter paper
    /ColorSpace /DeviceRGB def  %  RGB color space
    /Function Functiondict def  %  function to use for interpolation
end                             %  shading dictionary off of dictionary
stack

Patterndict begin               %  pattern dictionary to dictionary
stack
    /PatternType 2 def          %  pattern type 2 is a shading pattern
    /Shading Shadingdict def    %  shading dictionary
end                             %  pattern dictionary off of dictionary
stack

gsave                           %  save graphics state
    Patterndict                 %  pattern dictionary to operand stack
    [ 1 0 0 1 0 0 ]             %  identity matrix to operand stack
    makepattern                 %  make the pattern instance
    setpattern                  %  set that instance as current color
                                %  make a large font
    %% End external work

    k22path
    fill
    grestore
}def

