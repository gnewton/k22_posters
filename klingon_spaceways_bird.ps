<< /AllowTransparency true >> % enable transparency
<< /PageSize [792 1224] >> setpagedevice
%true setstrokeadjust

/pw currentpagedevice /PageSize get 0 get def
/ph currentpagedevice /PageSize get 1 get def


%% colours
/backgroundBlue{.613 .641 .75 setrgbcolor}def
/slotBlack{.079 .109 .136 setrgbcolor}def
/slotGray{.869 .884 .896 setrgbcolor}def


/leftWingWhite{.965 .965 .965 setrgbcolor}def
/rightWingDarkGray{.235 .251 .263 setrgbcolor}def

/leftArcUpperGray{.795 .823 .852 setrgbcolor}def
/leftArcLowerGray{.81 .84 .86 setrgbcolor}def

/rightArcUpperGray{.66 .64 .63 setrgbcolor}def
/rightArcLowerGray{.78 .76 .73 setrgbcolor}def

/leftSideK22{.922 .927 .920 setrgbcolor}def
/rightSideK22{.715 .737 .794 setrgbcolor}def

% test colours
/whiteLetterColor{1 1 .98 setrgbcolor}def

/cx 0 def
/cy 0 def

/wingWidth{8.25} def

/birdOutline{
    % beak
    % left beak
    newpath
    .1 setlinewidth
    %    cx 5.25 sub 203 moveto
    cx cy moveto
    %5.25 9.2  rlineto
    % right beak
    5.25 -9.2  rlineto
    % right neck
    3 -17  rlineto
    % top right wing
    21.75 5 rlineto
    % right outside wing
    41 -46 rlineto % slope=0.8913

    % outside of slot 2
    -8.05 0 rlineto
    % slot 2
    -9.8 11  rlineto
    -1.4 0 rlineto
    9.8 -11  rlineto
    -8.05 0 rlineto
    % slot 1
    -9.8 11  rlineto
    -1.4 0 rlineto
    9.8 -11  rlineto
    -8.05 0 rlineto
    % slot 0
    -9.8 11  rlineto
    -1.4 0 rlineto
    9.8 -11  rlineto
    % Wing divot, right
    -9.8 0  rlineto
    -6.5 14 rlineto
    -15.5 -.75 rlineto
    %% -9 0 rlineto ANGLED DIVOT
    -5.5 -14 rlineto

    % Tail, right
    5.25 -12.5 rlineto
    -10.6 -10.5 rlineto
%    cx 121.35 lineto

    %%%%%%%%%%%% centre %%%%%%%%%%%%%%%%%%%%
    % tail, left
    -10.5 10.5 rlineto
    5.25 12.5 rlineto

    % Wing divot, left
    -5.5 14 rlineto
    -15.5 .75 rlineto
    -6.5 -14 rlineto
    
    % slot 0
    -9.8 0  rlineto
    9.8 11  rlineto
    -1.4 0 rlineto
    -9.8 -11  rlineto
    % slot 1
    -8.05 0 rlineto
    9.8 11  rlineto
    -1.4 0 rlineto
    -9.8 -11  rlineto
    % slot 2n
    -8.05 0 rlineto
    9.8 11  rlineto
    -1.4 0 rlineto
    -9.8 -11  rlineto
    % outside of slot 2
    -8.05 0 rlineto

    % left outside wing
    41 46 rlineto

    % top left wing
    21.75 -5 rlineto

    % left neck
        3 17  rlineto
        closepath

}def % End bird

/topBlock{
    gsave
    newpath
    0 ph moveto
    pw 0 rlineto
    0 -74 rlineto
    pw -1 mul 0 rlineto
    closepath
    .931 .914 .903 setrgbcolor
    fill
    grestore


}def

/bottomBlock{
    gsave
    newpath
    0 0 moveto
    pw 0 rlineto
    0 300 rlineto
    pw -1 mul 0 rlineto
    closepath
    .91 .91 .91 setrgbcolor
    fill
    grestore

    gsave
    /Hershey-Plain-Duplex-Bold findfont 170 scalefont setfont
    whiteLetterColor
    0 10 moveto
    .65 1 scale
    4 0 (KLINGONSPACEWAYS) ashow
    grestore

    gsave
    /Bookman-Demi findfont 35 scalefont setfont
    ph .09 mul  ph .04 mul moveto
%    .95 1.30 scale
    .4 .4 .4 setrgbcolor
    3.5 0 (BORETH  &  BETA LANKAL) ashow
    grestore

    /Times-Bold findfont 100 scalefont setfont
    .68 .20 .15 setrgbcolor
    95 205 moveto
    gsave
    1.2 1 scale
    3 0 (KLINGON) ashow
    grestore

    .579 .619 .705 setrgbcolor
    90 110 moveto
    gsave
    -2.5 0 (SPACEWAYS) ashow
    grestore

    gsave
    /Hershey-Plain-Duplex-Bold findfont 170 scalefont setfont
    whiteLetterColor
    0 10 moveto
    .65 1 scale
    0.5 .setfillconstantalpha
    4 0 (KLINGONSPACEWAYS) ashow
    grestore


}def

/tailSlots{
        % right tail slots
    gsave
    newpath
    .1 setlinewidth
    cx 8.5 add cy 82.5 sub moveto
    % tail
    % tail slot 0
    -3.68 8.5 rlineto
    -.8 -.8 rlineto
    3.68 -8.5 rlineto
    currentpoint /y exch def /x exch def
    closepath
    slotBlack
    fill
    x y moveto
    -2 -2 rmoveto
    % tail slot 1
    -3.68 8.5 rlineto
    -.8 -.8 rlineto
    3.68 -8.5 rlineto
    closepath
    fill
    %
    grestore

    % left tail slots
    gsave
    slotGray
    newpath
    .1 setlinewidth
    cx 8.5 sub cy 82.45 sub moveto
    % tail
    % tail slot 0
    3.68 8.5 rlineto
    .8 -.8 rlineto
    -3.68 -8.5 rlineto

    currentpoint /y exch def /x exch def
    closepath
    fill
    x y moveto
    2 -2 rmoveto
    % tail slot 1
    3.68 8.5 rlineto
    .8 -.8 rlineto
    -3.68 -8.5 rlineto
    closepath
    fill
    %
    grestore
}def

/rightUpperArcClip{
    newpath
    cx 30.8 moveto
    wingWidth 0 rlineto
    250 -50 rlineto
    0 -50 lineto
    closepath
}def

/leftUpperArcClip{
    newpath
    cx 30.8 moveto
    wingWidth -1 0 rlineto
    -250 -50 rlineto
    0 -50 lineto
    closepath
}def

/rightArc{
    newpath
    cx 30.8 moveto
    wingWidth 0 rlineto
    250 -1000 rlineto
    0 -1000 lineto
    closepath
}def

/leftArc{

    
    newpath
    cx 30.8 moveto
    wingWidth -1 mul 0 rlineto
    -250 -1000 rlineto
    0 -1000 lineto
    closepath
}def

/middleBlock{
    gsave
    610 915 translate
    -33 rotate
    5.75 5.75 scale

    % left side
    gsave
    k22-arc-top-clip clip
    leftArc leftArcUpperGray fill
    grestore
    gsave
    k22-arc-bottom-clip clip
    leftArc leftArcLowerGray fill
    grestore
    
    gsave
    birdOutline 0 0 0 setrgbcolor fill
    leftArc clip
    birdOutline  leftWingWhite fill
    grestore

    % right side
    gsave
    k22-arc-top-clip clip
    rightArc rightArcUpperGray fill
    grestore
    gsave
     k22-arc-bottom-clip clip
    rightArc rightArcLowerGray fill
    grestore 
    gsave
    rightArc clip
    birdOutline rightWingDarkGray fill
    grestore

    gsave
    tailSlots
    grestore



    %.06 .06 scale
    gsave
    .05 .05 scale    
    -218.5 210 translate 

    leftSideK22
    k22 fill
    k22-left-clip clip %1 0 0 setrgbcolor stroke
    rightSideK22
    k22 fill
    grestore


    grestore % final

}def

/k22-left-clip{
    newpath
    218 160 moveto
    500 0 rlineto
    0 500  rlineto
    -500 0 rlineto
    closepath

}def

/k22-arc-clip{
    /size{8000}def
    /hlf{size 2 div}def
    newpath
    %outer box
    hlf -1 mul hlf -1 mul moveto
    size 0 rlineto
    0 size rlineto
    size -1 mul 0 rlineto
    closepath
    %inner box
     cx  30.95 moveto
     wingWidth .2 sub 0 rlineto
     0 -5 rlineto
     wingWidth -1 mul  -2 rlineto
     wingWidth -1 mul .4 add 2 rlineto
     0 5 rlineto
     closepath
}def

/k22-arc-top-clip{
    /size{75}def
    /hlf{size 2 div}def
    newpath
    %outer box
    hlf -1 mul hlf -1 mul moveto
    size 0 rlineto
    0 size rlineto
    size -1 mul 0 rlineto
    closepath
    %inner box
     cx  30.95 moveto
     wingWidth .2 sub 0 rlineto
     0 -5 rlineto
     wingWidth -1 mul  -2 rlineto
     wingWidth -1 mul .4 add 2 rlineto
     0 5 rlineto
     closepath
}def

/k22-arc-bottom-clip{
    /size{90}def
    /hlf{size 2 div}def
    newpath
    %outer box
    size -1 mul -50  moveto
    0 size -3 mul rlineto
    size 2 mul  0 rlineto
    0 size 3 mul rlineto
    closepath


}def


/background{
    gsave
    newpath
    0 0 moveto
    pw 0 rlineto
    0 ph rlineto
